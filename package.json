{
  "name": "tauri-vite-demo",
  "private": true,
  "version": "0.0.2",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "commit": "git add . && npm lint && git cz",
    "eslint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "dev": "vite",
    "format": "prettier --write ./**/*.{vue,ts,tsx,js,jsx,css,less,scss,json,md}",
    "genlog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint": "lint-staged",
    "preview": "vite preview",
    "prepare": "husky install",
    "tauri": "tauri",
    "release": "standard-version",
    "release-minor": "standard-version --release-as minor",
    "release-patch": "standard-version --release-as patch"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-zh"
    }
  },
  "lint-staged": {
    "*.{js,vue,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{vue,ts,tsx,js,jsx,css,less,scss,json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.1",
    "axios": "^0.27.2",
    "element-plus": "^2.2.9",
    "mockjs": "^1.1.0",
    "pinia": "^2.0.15",
    "qs": "^6.11.0",
    "tauri-plugin-serialport-api": "https://github.com/lzhida/tauri-plugin-serialport.git#master",
    "vue": "^3.2.25",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@tauri-apps/cli": "^1.0.2",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "@vitejs/plugin-vue": "^2.3.3",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.2.2",
    "cz-conventional-changelog-zh": "^0.0.2",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.1.1",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "standard-version": "^9.5.0",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.9.2",
    "unplugin-vue-components": "^0.21.1",
    "vite": "^2.9.9",
    "vite-plugin-mock": "^2.9.6",
    "vue-tsc": "^0.34.7"
  }
}
